algoritmo "Operadores"
var
   n1, n2, s: inteiro
   m: real
inicio
       escreva("infrome um numero:")
       leia(n1)
       escreva("informe outro numero:")
       leia(n2)
       s <- n1+ n2
       m <- (n1 + n2) / 2
       escreva("A média de ",n1,  " +", n2, " que é igual a: ", s ," dividido por 2 que é igual ", m)
fimalgoritmo